extends  ../_skeleton

block variables
	- var activeIndex = 4

block title
	title Site Map

block page
	include ../mixins/_breadcrumb

	// Page Header
	include ../sections/_header-default

	// Page Content
	main.page-content
		+breadcrumbs('images/breadcrumbs-04.jpg', 'Site Map', 'All Pages of Our Website',
		{
			0: {
				"title": "Home",
				"href": "./"
			},
			1: {
				"title": "Pages",
				"href": "#"
			},
			2: {
				"title": "Site Map"
			}
		}
		)
		// Privacy Policy
		section.text-md-left.section-80.section-md-120
			.shell.shell-wide
				.range.text-left
					.cell-sm-6.cell-md-3
						.inset-xl-right-60
							h5.hr-title.text-bold
								a(href='index.html').text-info-dr.text-info-dr Home
							
					.cell-sm-6.cell-md-3
						.inset-xl-right-60
							h5.hr-title.text-bold About
							ul.list-marked
								li
									a(href='overview.html').text-info-dr Overview
								li
									a(href='testimonials.html').text-info-dr Testimonials
								li
									a(href='faq.html').text-info-dr FAQ Page
					.cell-sm-6.cell-md-3.offset-top-50.offset-md-top-0
						.inset-xl-right-60
							h5.hr-title.text-bold Blog
							ul.list-marked
								li
									a(href='blog-classic.html').text-info-dr Classic Blog
								li
									a(href='blog-grid.html').text-info-dr Grid Blog
								li
									a(href='blog-masonry.html').text-info-dr Masonry Blog
								li
									a(href='blog-modern.html').text-info-dr Modern Blog
								li
									a(href='blog-single-post.html').text-info-dr Blog Post
					.cell-sm-6.cell-md-3.offset-top-50.offset-md-top-0
						.inset-xl-right-60
							h5.hr-title.text-bold
								a(href='contacts.html').text-info-dr.text-info-dr Contacts
					.cell-md-9.offset-lg-top-90
						.inset-xl-right-60
							h5.hr-title.text-bold.text-base Pages
							.range.offset-top-0
								.cell-sm-6.cell-md-4
									ul.list-marked
										li
											a(href='404.html').text-info-dr 404 Page
										li
											a(href='503.html').text-info-dr 503 Page
										li
											a(href='maintenance.html').text-info-dr Maintenance
										li
											a(href='soon.html').text-info-dr Coming Soon
										li
											a(href='search-results.html').text-info-dr Search Results

								.cell-sm-6.cell-md-4.offset-top-20.offset-sm-top-0
									ul.list-marked
										li
											a(href='privacy.html').text-info-dr Terms of Use
										li
											a(href='gallery-grid.html').text-info-dr Grid Gallery
										li
											a(href='gallery-masonry.html').text-info-dr Masonry Gallery
										li
											a(href='gallery-cobbles.html').text-info-dr Cobbles Gallery
										li
											a(href='gallery-gallery-full.html').text-info-dr Grid without Padding Gallery

								.cell-sm-6.cell-md-4.offset-top-20.offset-md-top-0
									ul.list-marked
										li
											a(href='tickets.html').text-info-dr Tickets List
										li
											a(href='login.html').text-info-dr Login/Registration
										li
											a(href='sitemap.html').text-info-dr Site Map

	// Page Footer
	include ../sections/_footer-default